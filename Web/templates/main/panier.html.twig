{% extends 'base.html.twig' %}

{% block title %}BDE CESI LILLE{% endblock %}
{% block stylesheets %}
<link href="style.css" rel="stylesheet" >
{% endblock %}

{% block body %}
<!--Affiche les éléments du panier sauvegardées en cookies-->
<h1>Le panier</h1><br>
<div class="panier">
    {{ app.request.cookies.get('Nom') }} <br><br>
    {{ app.request.cookies.get('Prix') }} <br><br>
    {{ app.request.cookies.get('Description') }}
</div>
{% endblock %}